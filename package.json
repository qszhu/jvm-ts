    "@types/node": "^14.14.16",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.16.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
